<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homeTabContainerVM" type="com.project.app.modules.hometabcontainer.data.viewmodel.HomeTabContainerVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnlogoOneOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_6pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowlogooneOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_22pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLogooneOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_59pxh"
                    android:layout_width="@dimen/_68pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logo_1_59x68}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logo_1_59x68"/>
                <FrameLayout android:id="@+id/frameStacktwentyfour"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxv"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_87pxh"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_26pxh">
                    <ImageView android:id="@+id/imageTwentyfourOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_24_notifications"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_24_notifications"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/txtGroupseventytwo"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_2pxh"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingEnd="@dimen/_4pxh"
                        android:paddingStart="@dimen/_4pxh"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@{homeTabContainerVM.homeTabContainerModel.txtGroupseventytwo}"
                        style="@style/txtSolidRounded" tools:text="@string/lbl_5"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameBasketicon"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_25pxv"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_28pxh">
                    <ImageView android:id="@+id/imageThumbsupOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_thumbs_up"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_thumbs_up"/>
                    <TextView android:ellipsize="end"
                        android:gravity="center"
                        android:id="@+id/txtGroupseventyone"
                        android:layout_gravity="top|start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_2pxh"
                        android:paddingBottom="@dimen/_1pxv"
                        android:paddingEnd="@dimen/_4pxh"
                        android:paddingStart="@dimen/_4pxh"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@{homeTabContainerVM.homeTabContainerModel.txtGroupseventyone}"
                        style="@style/txtSolidRounded" tools:text="@string/lbl_3"/>
                </FrameLayout>
                <ImageView android:id="@+id/imageFavoriteOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_favorite_black_900_01"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_favorite_black_900_01"/>
                <ImageView android:id="@+id/imageRewindOne"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rewind"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rewind"/>
                <ImageView android:id="@+id/imageThumbsupThree"
                    android:layout_gravity="center_vertical"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_thumbs_up_black_900_01"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_thumbs_up_black_900_01"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearHome"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <com.google.android.material.tabs.TabLayout
                    android:gravity="center_horizontal"
                    android:id="@+id/tabLayoutTabview"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    app:tabMode="fixed"
                    app:tabSelectedTextColor="@color/blue_gray_800" app:tabTextColor="@color/blue_gray_400"/>
                <androidx.viewpager2.widget.ViewPager2
                    android:gravity="center_horizontal"
                    android:id="@+id/viewPagerViewpager"
                    android:layout_height="match_parent"
                    android:layout_weight="1" android:layout_width="match_parent"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
