<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearColumnlogoOneOne"
    style="@style/groupStylegray_50"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
android:orientation="vertical">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarToolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_6pxv"
        android:gravity="center_horizontal"
        app:contentInsetStart="0dp">

        <LinearLayout
            android:id="@+id/linearRowlogooneOne"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_22pxh"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginBottom="@dimen/_17pxv"
                android:gravity="center"
                android:textColor="@color/black"
                android:text="Hello : " />

            <FrameLayout
                android:id="@+id/frameStacktwentyfour"
                android:layout_width="@dimen/_26pxh"
                android:layout_height="@dimen/_25pxv"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginBottom="@dimen/_17pxv">

                <ImageView
                    android:id="@+id/imageTwentyfourOne"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_gravity="end"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_24_notifications"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_24_notifications" />

                <TextView
                    android:id="@+id/txtGroupseventytwo"
                    style="@style/txtSolidRounded"
                    android:layout_width="15dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|start"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:lineSpacingExtra="@dimen/_2pxh"
                    android:paddingStart="@dimen/_4pxh"
                    android:paddingTop="@dimen/_1pxv"
                    android:paddingEnd="@dimen/_4pxh"
                    android:paddingBottom="@dimen/_1pxv"
                    android:singleLine="true"
/>
            </FrameLayout>

            <FrameLayout
                android:id="@+id/frameBasketicon"
                android:layout_width="@dimen/_28pxh"
                android:layout_height="@dimen/_25pxv"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginBottom="@dimen/_17pxv">

                <ImageView
                    android:id="@+id/imageThumbsupOne"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_gravity="end"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_thumbs_up"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_thumbs_up" />

                <TextView
                    android:id="@+id/txtGroupseventyone"
                    style="@style/txtSolidRounded"
                    android:layout_width="15dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|start"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:lineSpacingExtra="@dimen/_2pxh"
                    android:paddingStart="@dimen/_4pxh"
                    android:paddingTop="@dimen/_1pxv"
                    android:paddingEnd="@dimen/_4pxh"
                    android:paddingBottom="@dimen/_1pxv"
                    android:singleLine="true"
 />
            </FrameLayout>

            <ImageView
                android:id="@+id/imageFavoriteOne"
                android:layout_width="@dimen/_24pxh"
                android:layout_height="@dimen/_24pxh"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginBottom="@dimen/_17pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_favorite_black_900_01"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_favorite_black_900_01" />

            <ImageView
                android:id="@+id/imageRewindOne"
                android:layout_width="@dimen/_18pxh"
                android:layout_height="@dimen/_18pxh"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_marginBottom="@dimen/_20pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_rewind"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_rewind" />

            <ImageView
                android:id="@+id/imageThumbsupThree"
                android:layout_width="@dimen/_20pxh"
                android:layout_height="@dimen/_12pxh"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_marginBottom="@dimen/_23pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_thumbs_up_black_900_01"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_thumbs_up_black_900_01" />

        </LinearLayout>

    </androidx.appcompat.widget.Toolbar>

    <ScrollView
        android:id="@+id/scrollViewScrollview"
        android:layout_width="3500dp"
        android:layout_height="0dp"
        android:layout_marginTop="@dimen/_49pxv"
        android:layout_weight="1"
        android:clipToPadding="false"
        android:fillViewport="true"
        android:gravity="center_horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:paddingRight="100dp"
            android:orientation="horizontal">

            <!-- First Horizontal RecyclerView -->

            <!-- Second Horizontal RecyclerView -->

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView1"
                android:layout_width="185dp"
                android:layout_height="match_parent"
                android:orientation="horizontal" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView2"
                android:layout_width="185dp"
                android:layout_height="match_parent"
                android:orientation="horizontal" />

        </LinearLayout>
    </ScrollView>
</LinearLayout>
