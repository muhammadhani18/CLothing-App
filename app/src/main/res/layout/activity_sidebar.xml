<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="sidebarVM" type="com.project.app.modules.sidebar.data.viewmodel.SidebarVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumncreateFromOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameStackcreatefrom"
                android:layout_height="@dimen/_811pxv"
                android:layout_marginStart="@dimen/_106pxh" android:layout_width="@dimen/_284pxh">
                <View android:background="@color/gray_900_01"
                    android:id="@+id/viewCreatefromOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_811pxh" android:layout_width="@dimen/_284pxh"/>
                <LinearLayout android:id="@+id/linearColumncloseOne"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_width="wrap_content" android:orientation="vertical">
                    <ImageView android:id="@+id/imageCloseOne"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_15pxh"
                        android:layout_marginEnd="@dimen/_134pxh"
                        android:layout_width="@dimen/_15pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_close_white_a700"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_close_white_a700"/>
                    <LinearLayout android:id="@+id/linearRowemail"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_134pxh"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_67pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageImagedefault"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_42pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_width="@dimen/_42pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_image_default"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_image_default"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnfullname"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_31pxh"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <LinearLayout
                                android:id="@+id/linearRowfullname"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtFullname"
                                    android:layout_height="wrap_content"
                                    android:layout_width="match_parent"
                                    android:lineSpacingExtra="@dimen/_4pxh"
                                    android:singleLine="true"
                                    android:text="@{sidebarVM.sidebarModel.txtFullname}"
                                    style="@style/txtWorkSansregular18" tools:text="@string/lbl_full_name"/>
                                <ImageView
                                    android:id="@+id/imageEditOne"
                                    android:layout_height="@dimen/_15pxh"
                                    android:layout_marginBottom="@dimen/_5pxv"
                                    android:layout_marginStart="@dimen/_31pxh"
                                    android:layout_width="@dimen/_15pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_edit"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_edit"/>
                            </LinearLayout>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtEmail"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="match_parent"
                                android:lineSpacingExtra="@dimen/_3pxh"
                                android:singleLine="true"
                                android:text="@{sidebarVM.sidebarModel.txtEmail}"
                                style="@style/txtWorkSansregular14_2" tools:text="@string/lbl_email_gmail_com"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowbagOne"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_58pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageBagOne"
                            android:layout_height="@dimen/_24pxh"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_bag"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_bag"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtOrders"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginStart="@dimen/_17pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{sidebarVM.sidebarModel.txtOrders}"
                            style="@style/txtWorkSansregular14" tools:text="@string/lbl_orders"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowtelevision"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_49pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageTelevisionOne"
                            android:layout_gravity="bottom"
                            android:layout_height="@dimen/_15pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_television"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_television"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtMydetails"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{sidebarVM.sidebarModel.txtMydetails}"
                            style="@style/txtWorkSansregular14" tools:text="@string/lbl_my_details"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearDelivery"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_36pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/linearRowlinkedinOne"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageLinkedinOne"
                                android:layout_gravity="center_vertical"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_width="@dimen/_16pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_linkedin"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_linkedin"/>
                            <TextView android:ellipsize="end"
                                android:gravity="top"
                                android:id="@+id/txtDelivery"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_3pxv"
                                android:layout_marginStart="@dimen/_26pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{sidebarVM.sidebarModel.txtDelivery}"
                                style="@style/txtWorkSansregular14" tools:text="@string/msg_delivery_address"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageArrowrightOne"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_196pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_8pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrow_right_gray_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowvectoricon"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_41pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVectoriconOne"
                            android:layout_gravity="bottom"
                            android:layout_height="@dimen/_13pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_vector_icon"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_icon"/>
                        <TextView android:ellipsize="end"
                            android:gravity="center_vertical"
                            android:id="@+id/txtPaymentmethods"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{sidebarVM.sidebarModel.txtPaymentmethods}"
                            style="@style/txtWorkSansregular14" tools:text="@string/lbl_payment_methods"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowtelevision1"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_43pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageTelevision"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_22pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_television_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_television_white_a700"/>
                        <TextView android:ellipsize="end"
                            android:gravity="top"
                            android:id="@+id/txtPromocord"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{sidebarVM.sidebarModel.txtPromocord}"
                            style="@style/txtWorkSansregular14" tools:text="@string/lbl_promo_cord"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearNotifecations"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_40pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/linearRowbelliconOne"
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageBelliconOne"
                                android:layout_gravity="bottom"
                                android:layout_height="@dimen/_20pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_width="@dimen/_17pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_bell_icon"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_bell_icon"/>
                            <TextView android:ellipsize="end"
                                android:gravity="top"
                                android:id="@+id/txtNotifecations"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginStart="@dimen/_25pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{sidebarVM.sidebarModel.txtNotifecations}"
                                style="@style/txtWorkSansregular14" tools:text="@string/lbl_notifecations"/>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageArrowright"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_14pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginStart="@dimen/_219pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="@dimen/_8pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrow_right_gray_900"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_arrow_right_gray_900"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowprofileOne"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_40pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageProfileOne"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_profile"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_profile"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHelp"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{sidebarVM.sidebarModel.txtHelp}"
                            style="@style/txtWorkSansregular14" tools:text="@string/lbl_help"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowvideocamera"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_41pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageVideocameraOne"
                            android:layout_gravity="center_vertical"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_video_camera"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_video_camera"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtAbout"
                            android:layout_gravity="top"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_3pxv"
                            android:layout_marginStart="@dimen/_24pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{sidebarVM.sidebarModel.txtAbout}"
                            style="@style/txtWorkSansregular14" tools:text="@string/lbl_about"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLogout"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_100pxv"
                        android:layout_width="wrap_content"
                        android:lineSpacingExtra="@dimen/_2pxh"
                        android:singleLine="true"
                        android:text="@{sidebarVM.sidebarModel.txtLogout}"
                        android:textAllCaps="true"
                        style="@style/txtWorkSansregular11" tools:text="@string/lbl_log_out"/>
                </LinearLayout>
            </FrameLayout>
        </ScrollView>
    </LinearLayout>
</layout>
